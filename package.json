{
  "name": "apex-omega-titan",
  "version": "4.2.0",
  "description": "Multi-Chain Arbitrage & Flash Loan Execution System with AI-Powered Intelligence",
  "main": "execution/bot.js",
  "scripts": {
    "start": "node execution/bot.js",
    "brain": "python3 ml/brain.py",
    "compile": "npx hardhat compile",
    "deploy:polygon": "npx hardhat run scripts/deploy.js --network polygon",
    "deploy:arbitrum": "npx hardhat run scripts/deploy.js --network arbitrum",
    "deploy:optimism": "npx hardhat run scripts/deploy.js --network optimism",
    "deploy:base": "npx hardhat run scripts/deploy.js --network base",
    "test": "npx hardhat test && python3 test_phase1.py",
    "audit": "python3 audit_system.py",
    "health": "redis-cli ping && node -v && python3 --version",
    "clean": "rm -rf artifacts cache typechain typechain-types",
    "setup": "npm install --legacy-peer-deps && pip3 install -r requirements.txt && npx hardhat compile"
  },
  "keywords": [
    "defi",
    "arbitrage",
    "flash-loans",
    "ethereum",
    "polygon",
    "blockchain",
    "trading-bot",
    "ai",
    "machine-learning",
    "cross-chain"
  ],
  "author": "MavenSource",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MavenSource/Titan.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@flashbots/ethers-provider-bundle": "^1.0.0",
    "@lifi/sdk": "^3.0.0",
    "@openzeppelin/contracts": "^5.4.0",
    "@paraswap/sdk": "^7.3.1",
    "axios": "^1.6.7",
    "dotenv": "^16.4.1",
    "ethers": "^6.16.0",
    "redis": "^4.6.12"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "hardhat": "^2.19.5"
  },
}
